            <form asp-action="Paylasim" asp-controller="Home" enctype="multipart/form-data" method="post">
                <input type="file" id="postsekilselect" asp-for="PostPhotoUrl">

                <div class="parea">
                    <!-- <div class="item">
                        <img src="audio.png">
                        <input type="file">
                    </div> -->
                </div>
                <input type="text" placeholder="Şərh" autocomplete="off" asp-for="PostDescription">
                <button class="btn btn-primary sekpaylas"><a class="text-light p">Paylas</a></button>
            </form>
        </div>
    </section>

    <script src="/Js/post.js"></script>
    </body>